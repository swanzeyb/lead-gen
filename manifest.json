{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Lead Reporter",
  "description": "",
  "icons": {
    "24": "public/icon_24.png"
  },
  "action": {
    "default_popup": "public/index.html",
    "default_title": "Lead Reporter"
  },
  "permissions": ["scripting", "tabs", "storage", "activeTab"],
  "host_permissions": ["https://*/*", "*://*/*"],
  "background": {
    "service_worker": "out/background.js",
    "type": "module"
  }
}
